#include <iostream>

using namespace std;

int main()
{
    int n;
    cout<<"enter the limit: ";
    cin>>n;

    if(n<0)
    {

        cout<<"Invalid input";
    }

    else
    {
        for(int i=0;i<n;i++)
        if(i%2==0)
        {
            cout<<i<<" ";
            i++;
        }


    }

    return 0;
}
